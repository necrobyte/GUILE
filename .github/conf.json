{
    "plugins": [ "plugins/commentConvert" ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.gml?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
